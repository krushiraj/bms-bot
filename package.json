{
  "name": "bms-bot",
  "version": "1.0.0",
  "description": "BookMyShow ticket booking automation service",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "bookmyshow",
    "automation",
    "telegram-bot",
    "playwright"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "bullmq": "^5.1.0",
    "dotenv": "^17.2.3",
    "fastify": "^4.25.0",
    "grammy": "^1.21.0",
    "ioredis": "^5.3.2",
    "playwright": "^1.40.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prisma": "^5.7.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  }
}
